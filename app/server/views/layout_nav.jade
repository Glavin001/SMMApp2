!!! 5
//-html(lang="en", manifest="/appcache")
html
	head
		title #{title}
		meta(name="viewport", content="width=device-width", initial-scale="1.0", maximum-scale="1.0", user-scalable="no") //- For Mobile
		meta(http-equiv="cache-control", content="max-age=0")
		meta(http-equiv="cache-control", content="no-cache")
		meta(http-equiv="expires", content="0")
		meta(http-equiv="expires", content="Tue, 01 Jan 1980 1:00:00 GMT")
		meta(http-equiv="pragma", content="no-cache")

		include vendors

		link(rel='stylesheet', href='/css/style.css')
		link(rel='stylesheet', href='/css/map.css')
		
		block header

	body.hasNavigation
		//-include github_banner
		include navbar
	
	include modals/alert
	include modals/confirm
	
	block content
	
	//- For Account management and logging out	
	script(src='/js/views/home.js')
	script(src='/js/controllers/homeController.js')
	script(src='/js/form-validators/accountValidator.js')

	block scripts
